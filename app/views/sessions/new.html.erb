<h1>Log In</h1>

<%= form_for @user do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Log In" %>
<% end %>
<br><br>
<p>Don't have an account? <%= link_to "Sign Up", new_user_path %></p> 
